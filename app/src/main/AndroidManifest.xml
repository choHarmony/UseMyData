<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hanium.ictapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ICTapp"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".ImageActivity"
            android:exported="true" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StayViewActivity"
            android:exported="true" />
        <activity
            android:name=".RecommendActivity"
            android:exported="true" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".AlarmActivity" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".ConsumeStep3Activity" />
        <activity android:name=".ConsumeStep1Activity" />
        <activity android:name=".EstateStep3Activity" />
        <activity android:name=".EstateStep1Activity" />
        <activity android:name=".ConsumeWishActivity" />
        <activity android:name=".StayRestartActivity" />
        <activity android:name=".InquiryActivity" />
        <activity android:name=".HelpActivity" />
        <activity android:name=".CheckStayActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".InfoActivity" />
        <activity android:name=".CommunityActivity" />
        <activity android:name=".EstateActivity" />
        <activity android:name=".ConsumeActivity" />
        <activity android:name=".StayActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>